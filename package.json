{
  "name": "@libersys/speex-resampler-napi",
  "version": "4.0.0",
  "description": "High-performance SpeexDSP resampler using N-API (revived from v1.0.14)",
  "gypfile": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "clean": "rimraf build dist",
    "compile": "tsc --project tsconfig.json",
    "native": "node-gyp rebuild",
    "build": "npm run native && npm run compile",
    "install": "npm run native",
    "prepublishOnly": "npm run build",
    "test": "npm run compile && node dist/test.js"
  },
  "author": "Guillaume Besson <guillaume@besson.co>",
  "repository": "https://github.com/geekuillaume/node-speex-resampler",
  "keywords": [
    "audio",
    "resampling",
    "pcm",
    "speex"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "node-addon-api": "^8.5.0",
    "node-gyp": "^12.1.0",
    "typescript": "^5.9.3"
  }
}
